				<paragraph leos:origin="ec" xml:id="akn_article_ebFhaW_S1g3RO">
					<num leos:origin="ec" xml:id="akn_article_ebFhaW_SkSGPr">1.</num>
					<subparagraph leos:origin="ec" xml:id="akn_article_ebFhaW_8vWzbq">
						<content leos:origin="ec" xml:id="akn_article_ebFhaW_oNWhzj">
							<p leos:origin="ec" xml:id="akn_article_ebFhaW_iMglHR">Test of indent-outdent of points-subpoints</p>
						</content>
					</subparagraph>
					<list leos:origin="ec" xml:id="akn_article_ebFhaW_11DoqG">
						<point leos:origin="ec" xml:id="akn_article_ebFhaW_65b9hw">
							<num leos:origin="ec" xml:id="akn_article_ebFhaW_4R6fnK">(a)</num>
							<content leos:origin="ec" xml:id="akn_article_ebFhaW_0hAjIK">
								<p leos:origin="ec" xml:id="akn_article_ebFhaW_xSp3z6">&apos;cultural goods&apos; means any item which is of importance for archaeology, prehistory, history, literature, art or science as listed in the Annex;</p>
							</content>
						</point>
						<point leos:origin="ec" xml:id="akn_article_ebFhaW_umO9jn">
							<num leos:origin="ec" xml:id="akn_article_ebFhaW_kkIn19">(b)</num>
							<content leos:origin="ec" xml:id="akn_article_ebFhaW_HbiCZh">
								<p leos:origin="ec" xml:id="akn_article_ebFhaW_t1gwQI">&apos;introduction of cultural goods&apos; means any entry into the customs territory of the Union of cultural goods which are subject to customs supervision or customs control within the customs territory of the Union in accordance with Regulation (EU) No 952/2013.</p>
							</content>
						</point>
						<point leos:origin="ec" xml:id="akn_article_ebFhaW_SoxNh2">
